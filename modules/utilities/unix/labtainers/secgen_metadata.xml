<?xml version="1.0"?>

<utility xmlns="http://www.github/cliffe/SecGen/utility"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.github/cliffe/SecGen/utility">
  <name>Labtainers: A Docker-based cyber lab framework</name>
  <author>Z. Cliffe Schreuders</author>
  <author>Mike Thompson - Naval Postgraduate School</author>
  <module_license>GPLv3</module_license>
  <description>Labtainers include more than 40 cyber lab exercises and tools to build your own.</description>

  <type>lab-infrastucture</type>
  <platform>linux</platform>

  <reference>https://my.nps.edu/web/c3o/labtainers</reference>
  <software_license>public domain</software_license>

  <read_fact>lab</read_fact>

  <default_input into="lab">
    <value>shellbasics</value>
  </default_input>


  <requires>
    <type>update</type>
  </requires>
</utility>
